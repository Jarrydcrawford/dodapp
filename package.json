{
  "name": "dodapp",
  "version": "0.1.0",
  "description": "Dodapp makes it easier to collect DOD and MOM votes, without losing any of the crudeness or mockery.",
  "author": "Jarryd Crawford <jarrydcrawford@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=8",
    "yarn": ">=1.3"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link-http": "^1.5.3",
    "catling": "^1.4.0",
    "emotion": "^9.2.10",
    "formik": "^1.3.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "qs": "^6.5.2",
    "react": "^16.5.2",
    "react-apollo": "2.1.11",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.10",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "semantic-ui-css": "^2.4.0",
    "semantic-ui-react": "^0.82.5"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build && cp ./build/index.html ./build/404.html",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "types": "CODEGEN_AVOID_OPTIONALS=true CODEGEN_ENUMS_AS_TYPES=true gql-gen --schema ${REACT_APP_API_URL}  --template graphql-codegen-typescript-template --out ./src/types/gql.ts"
  },
  "devDependencies": {
    "@types/graphql": "^0.13.0",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.10.1",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-router-dom": "^4.3.1",
    "graphql-code-generator": "^0.12.6",
    "graphql-codegen-typescript-template": "^0.12.6",
    "prettier": "^1.14.3",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.2",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-styled-components"
    ]
  }
}
